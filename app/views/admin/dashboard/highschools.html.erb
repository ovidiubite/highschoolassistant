<h3> Highschools </h3>
<table data-role="table" class=" table ui-responsive">
    <thead>
        <tr>
            <th width="80%">Name</th>
            <th>County</th>
        </tr>
    </thead>

    <tbody class="aling-left">
        <% @highschools.each do |highschool| %>
            <tr>
                <td width="80%"><%= link_to highschool.name, admin_highschool_details_path(highschool.id) %> </td>
                <td><%= highschool.county.name %></td>
            </tr>
        <% end %>
    </tbody>
</table>
<%= will_paginate @highschools, renderer: BootstrapPagination::Rails %>
